{
  "name": "loyep/prism-framework",
  "description": "A wonderful administration panel.",
  "keywords": [
    "laravel",
    "admin"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Loyep",
      "email": "mwl@live.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "erusev/parsedown": "^1.7",
    "erusev/parsedown-extra": "^0.7.1",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^5.8",
    "intervention/image": "^2.4",
    "predis/predis": "^1.1",
    "qiniu/php-sdk": "^7.2",
    "tymon/jwt-auth": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Loyep\\Prism\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Loyep\\Prism\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Loyep\\Prism\\Providers\\PrismServiceProvider"
      ],
      "aliases": {
        "Prism": "Loyep\\Prism\\Facades\\Prism"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
